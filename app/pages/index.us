<!DOCTYPE html>
<html ng-app="app" lang="en">
  <head>
    <title><%= pkg.name %></title>
    <link rel="stylesheet" type="text/css" href="/css/app.css" media="all" />
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
  </head>
  <body>
    <div class="row">
      <div style="width: 100%; height:100%; overflow: hidden;">



        <div class="cube-container">
          <div class="cube cube-gap">
            <figure class="side top"><a href="http://www.apple.com"></a></figure>
            <figure class="side left"><a href="http://www.apple.com"></a></figure>
            <figure class="side right"><a href="http://www.apple.com">DJ Replay</a></figure>
          </div>
          <div class="cube">
            <figure class="side top"><a href="http://www.apple.com"></a></figure>
            <figure class="side left"><a href="http://www.apple.com">Now Playing</a></figure>
            <figure class="side right"><a href="http://www.apple.com"></a></figure>
          </div>




          <div class="cube">
            <figure class="side top"><a href="http://www.apple.com"></a></figure>
            <figure class="side left"><a href="http://www.apple.com"></a></figure>
            <figure class="side right"><a href="http://www.apple.com"></a></figure>
          </div>
          <div class="cube">
            <figure class="side top red"><a href="http://www.apple.com">record icon</a></figure>
            <figure class="side left purple"><a href="http://www.apple.com">person icon</a></figure>
            <figure class="side right blue"><a href="http://www.apple.com">drink icon</a></figure>
          </div>
          <div class="cube">
            <figure class="side top"><a href="http://www.apple.com"></a></figure>
            <figure class="side left"><a href="http://www.apple.com"></a></figure>
            <figure class="side right"><a href="http://www.apple.com"></a></figure>
          </div>


          <div class="cube cube-gap">
            <figure class="side top"><a href="http://www.apple.com">Ice Breaker</a></figure>
            <figure class="side left"><a href="http://www.apple.com"></a></figure>
            <figure class="side right"><a href="http://www.apple.com"></a></figure>
          </div>
          <div class="cube">
            <figure class="side top"><a href="http://www.apple.com">Drink Link</a></figure>
            <figure class="side left"><a href="http://www.apple.com"></a></figure>
            <figure class="side right"><a href="http://www.apple.com"></a></figure>
          </div>
          <div class="cube">
            <figure class="side top"><a href="http://www.apple.com"></a></figure>
            <figure class="side left"><a href="http://www.apple.com"></a></figure>
            <figure class="side right"><a href="http://www.apple.com"></a></figure>
          </div>

          <div class="cube">
            <figure class="side top"><a href="http://www.apple.com">this one</a></figure>
            <figure class="side left"><a href="http://www.apple.com"></a></figure>
            <figure class="side right"><a href="http://www.apple.com"></a></figure>
          </div>
          <div class="cube">
            <figure class="side top"><a href="http://www.apple.com"></a></figure>
            <figure class="side left"><a href="http://www.apple.com"></a></figure>
            <figure class="side right"><a href="http://www.apple.com"></a></figure>
          </div>

      </div>
      </div>
    </div>



    <div class="row">
      <div class="large-12" ng-controller="PanelController as panel">
        <ul class="nav nav-pills">
            <li ng-class="{active: panel.isSelected(1)}"><a href ng-click="panel.selectTab(1)"> One </a></li>
            <li ng-class="{active: panel.isSelected(2)}"><a href ng-click="panel.selectTab(2)"> Two </a></li>
            <li ng-class="{active: panel.isSelected(3) }"><a href ng-click="panel.selectTab(3)"> Three </a></li>
        </ul>
        <div>
          <div class="panel" ng-show="panel.isSelected(1)">
            <p>Panel 1</p>
          </div>
          <div class="panel" ng-show="panel.isSelected(2)">
            <p>Panel 2</p>
          </div>
          <div class="panel" ng-show="panel.isSelected(3)">
            <p>Panel 3</p>
          </div>
        </div>
        <div id="view" ng-view></div>
      </div>
    </div>

    <script type="text/javascript" src="/js/app.js"></script>
  </body>
</html>
